    image: mysql:8.0.25
    ports:
    - published: 3366
      target: 3306
    restart: always
    volumes:
    - journey-datafiles:/var/lib/mysql:rw
  JourneyMicroservice:
    build:
      context: C:\Users\Emanueld92\source\repos\Crud_V2\Unified
      dockerfile: JourneyMicroservice.Api/Dockerfile
