services:
  Journey-database:
    environment:
      MYSQL_DATABASE: journey
      MYSQL_PASSWORD: password
      MYSQL_ROOT_PASSWORD: root
      MYSQL_USER: emanueld92
    image: mysql:8.0.25
    ports:
    - published: 3366
      target: 3306
    restart: always
    volumes:
    - journey-datafiles:/var/lib/mysql:rw
  JourneyMicroservice:
    build:
      context: C:\Users\Emanueld92\source\repos\Crud_V2\Unified
      dockerfile: JourneyMicroservice.Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: JourneyMicroservice.Api
      target: base
    container_name: JourneyMicroservice.Api
    depends_on:
      Journey-database:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      Journey-database: null
      NUGET_FALLBACK_PACKAGES: ''
    image: JourneyMicroservice:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  ""'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5014
      target: 80
    - target: 80
    - published: 732
      target: 443
    - target: 443
    restart: always
    tty: true
    volumes:
    - C:\Users\Emanueld92\source\repos\Crud_V2\Unified\JourneyMicroservice.Api:/app:rw
    - C:\Users\Emanueld92\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Emanueld92\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Emanueld92\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Emanueld92\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\Emanueld92\source\repos\Crud_V2\Unified:/src:rw
